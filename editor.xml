<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="PREVIEW" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bnote\b.*">
      <option name="FOREGROUND" value="fcff1b" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bDEV\b.*">
      <option name="FOREGROUND" value="41ffdc" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <filter name="TODO">
      <pattern index="1" />
      <pattern index="0" />
    </filter>
    <filter name="DEV">
      <pattern index="3" />
    </filter>
  </component>
</application>